<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wenqinghao.ordeform.mapper.OrderGoodsMapper">
  <resultMap id="BaseResultMap" type="com.wenqinghao.ordeform.domain.entity.OrderGoods">
    <!--@mbg.generated-->
    <!--@Table order_goods-->
    <result column="o_id" jdbcType="INTEGER" property="oId" />
    <result column="g_id" jdbcType="INTEGER" property="gId" />
    <result column="quantity" jdbcType="INTEGER" property="quantity" />
  </resultMap>

  <!---->
  <select id="getListGoodsByOId" resultMap="BaseResultMap">
    select g_id,quantity
    from order_goods
    where o_id = #{oId}
  </select>
</mapper>